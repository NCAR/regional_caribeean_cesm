                                ! A minium wave speed used in the Ferrari et al., 2010, streamfunction
                                ! formulation.
USE_KH_IN_MEKE = True           !   [Boolean] default = False
                                ! If true, uses the thickness diffusivity calculated here to diffuse MEKE.

